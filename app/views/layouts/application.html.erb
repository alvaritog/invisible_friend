<!DOCTYPE html>
<html>
<head>
  <title>InvisibleFriend</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div id="content-wrapper"  class="front-end">

    <div id="header">
      <div id="header_content">
        <h1><%= link_to 'Amigo invisible', root_path %></h1>
        <div class="user-auth-nav">
          <%= render "shared/user_nav" %>
        </div>
      </div>
    </div>

  <%= render "shared/flash_messages" %>
      
    <%= yield %>

    <div id="footer">
      <div id="footer_content">
        <p>This is the footer!</p>
      </div>
    </div>

  </div>


</body>
</html>
